rm server
rm server.o
rm server_rss
rm server_rss.o
rm server_locking
rm server_locking.o

g++ -fpermissive -g -I ../../include -I /home/hss/Downloads/MTP/netmap-master/sys -std=c++11 -o server.o -c server.cpp
g++ -fpermissive -g -I ../../include -I /home/hss/Downloads/MTP/netmap-master/sys -std=c++11 -o server_rss.o -c server_rss.cpp
g++ -fpermissive -g -I ../../include -I /home/hss/Downloads/MTP/netmap-master/sys -std=c++11 -o server_locking.o -c server_locking.cpp
echo "Server done"
g++ -o server server.o ../../mtcp_lib.o -pthread
g++ -o server_rss server_rss.o ../../mtcp_lib.o -pthread
g++ -o server_locking server_locking.o ../../mtcp_lib.o -pthread
