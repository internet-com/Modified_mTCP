rm client
rm client.o
rm client_rss
rm client_rss.o
rm client_app
rm client_app.o

g++ -g -fpermissive -I ../../include  -I /home/hss/Downloads/MTP/netmap-master/sys  -std=c++11 -o client.o -c client.cpp
g++ -g -fpermissive -I ../../include  -I /home/hss/Downloads/MTP/netmap-master/sys  -std=c++11 -o client_rss.o -c client_rss.cpp
g++ -g -fpermissive -I ../../include   -I /home/hss/Downloads/MTP/netmap-master/sys  -std=c++11 -o client_app.o -c client_app.cpp
echo "Client done"
g++ -o client client.o ../../mtcp_lib.o -pthread
g++ -o client_rss client_rss.o ../../mtcp_lib.o -pthread
g++ -o client_app client_app.o ../../mtcp_lib.o -pthread
